#%RAML 1.0 Library
uses:
  annotations: ../../../../../../glapi-global-apis-commons-commons/annotations/annotationsLibrary.raml

types:
  contributor:
    type: object
    description: "Contributor related on the form."
    properties:
      id:
        type: string
        description: |
          Identifier associated to the contributor.
        required: true

      name:
        type: string
        description: |
          Name associated to the contributors.
        required: false

