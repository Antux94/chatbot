#%RAML 1.0 Library
uses:
  annotations: ../../../../../../../glapi-global-apis-commons-commons/annotations/annotationsLibrary.raml

types:
  value:
    type: object
    description: "Value of the house in which the participant resides, if it is leased it will be the rental value, if the house is owned, it will be the value of the property."
    properties:
      amount:
        type: number
        description: |
          Residence value.
        required: true

      currency:
        type: string
        description: |
          String based on ISO-4217 for specifying the currency related to the amount.
        required: true

