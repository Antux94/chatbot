#%RAML 1.0 Library
uses:
  annotations: ../../../../../../../glapi-global-apis-commons-commons/annotations/annotationsLibrary.raml

types:
  patrimony:
    type: object
    description: "Stores the equity owned by the participant."
    properties:
      amount:
        type: number
        description: |
          Value associated with patrimony.
        required: true

      currency:
        type: string
        description: |
          String based on ISO-4217 for specifying the currency related to the amount.
        required: true

