#%RAML 1.0 Library
uses:
  annotations: ../../../../../../../glapi-global-apis-commons-commons/annotations/annotationsLibrary.raml

types:
  bankDepositBalance:
    type: object
    description: "Deposit that the participant has in the bank."
    properties:
      amount:
        type: number
        description: |
          Deposit value.
        required: true

      currency:
        type: string
        description: |
          String based on ISO-4217 for specifying the currency related to the amount.
        required: true

